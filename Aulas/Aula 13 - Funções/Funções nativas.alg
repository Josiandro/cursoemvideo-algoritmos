algoritmo "Funções nativas do Visualg"
var
   site: caracter
inicio
      site <- "CursoEmVideo"
      Escreval(Compr(site))
      Escreval(Copia(site, 6, 2))
      Escreval(Maiusc(site))
      Escreval(Minusc(site))
      Escreval(Pos("Video", site))
      Escreval(Asc("C"))
      Escreval(Carac(67))
fimalgoritmo
